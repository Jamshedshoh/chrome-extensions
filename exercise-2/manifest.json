{
    "manifest_version": 2,
    "name": "Name",
    "description": "Description",
    "version": "1.0",
    "icons": {
        "16": "./icon.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.google.com/*",
        "storage"
    ]
}